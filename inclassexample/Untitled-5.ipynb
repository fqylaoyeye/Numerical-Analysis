   "source": [
    "function backward_substitution(U::Matrix, b::Vector)\n",
    "    n = size(U, 1)\n",
    "    x = similar(b)\n",
    "    x[n] = b[n] / U[n, n]\n",
    "    for i in n-1:-1:1\n",
    "        x[i] = (b[i] - dot(U[i, i+1:n], x[i+1:n])) / U[i, i]\n",
    "    end\n",
    "    return x\n",
    "end"
   ]
  }
