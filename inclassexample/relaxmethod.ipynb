   "source": [
    "function try_to_relax(A,b,x,ϵ,ω)\n",
    "    n=length(x)\n",
    "    N=[(i==j) ? 0 : -A[i,j] for i in 1:n, j in 1:n]\n",
    "    while norm(A * x - b) / norm(b) > ϵ\n",
    "        for i in 1:N\n",
    "            x[i] = (N[i,:]' * x + b[i]) / A[i,i]\n",
    "        end\n",
    "    end\n",
    "    return x\n",
    "end"
   ]
