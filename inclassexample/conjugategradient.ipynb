   "source": [
    "function conjugate_gradients(A,x,b,ϵ)\n",
    "∇f(x)=A*x-b\n",
    "let d=∇f(x)\n",
    "    while ∇f(x)'∇f(x)>=ϵ*b'b\n",
    "        x-=∇f(x)'d/(d'A*d)*d\n",
    "        d=∇f(x)-∇f(x)'A*d/(d'A*d)*d\n",
    "    end\n",
    "end\n",
    "return x\n",
    "end\n",
    "\n",
    "A=[2 1;1 2]\n",
    "sol=[2;3]\n",
    "b=A*sol\n",
    "conjugate_gradients(A,[0,0],b,10^-3)\n"
   ]
  }
