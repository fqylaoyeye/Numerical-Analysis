    "function steepest_descent(A,x,b,ϵ)\n",
    "    ∇f(x)=A*x-b\n",
    " while ∇f(x)'∇f(x)≥ ϵ * b'b\n",
    " x -=∇f(x)'∇f(x)/(∇f(x)'A*∇f(x)) * ∇f(x)\n",
    " end\n",
    "return x\n",
    "end\n",
    "\n",
    "A=[2 1;1 2]\n",
    "sol=[2;3]\n",
    "b=A*sol\n",
    "steepest_descent(A,[0,0],b,10^-3)"
   ]
  }
