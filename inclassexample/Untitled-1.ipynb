    "using LinearAlgebra\n",
    "using Statistics\n",
    "\n",
    "χ(x)= norm(x) < 1\n",
    "N= 10^7\n",
    "dim= 2\n",
    "X=rand(N,dim)\n",
    "Y=[2^dim * χ(X[i,:]) for i in 1:N]\n",
    "I=sum(Y)/N\n",
    "θ=std(Y)\n",
    "I,θ\n"
   ]
  }
